<ul>
  <li>Title</li>
  <dd><%= @calendar_item.title %></dd>

  <li>Body</li>
  <dd><%= @calendar_item.body %></dd>

  <li>Start Date</li>
  <dd><%= @calendar_item.start_time %></dd>

  <li>End Date</li>
  <dd><%= @calendar_item.end_time %></dd>
</ul>

<%= button_to("Edit Event", edit_admin_calendar_item_url(@calendar_item), method: :get) %>

<%= button_to("Delete Event", admin_calendar_item_path(@calendar_item), method: :delete) %>
